{
		"name": "Go Anonymous!",
		"version": "1.0.2",
		"description": "When you want an uninterrupted place to be.",
		"manifest_version": 2,
		"author": "Swateek Jena",
		"homepage_url": "https://github.com/swateek/go-anonymous",
		"background": {
			"scripts": ["background.js"],
			"persistent": false
		},
		"browser_action": {
			"default_icon": {
				"16": "images/go_anon_16.png",
				"24": "images/go_anon_24.png",
				"32": "images/go_anon_32.png",
				"48": "images/go_anon_48.png",
				"128": "images/go_anon_128.png"
			}
		},
		"icons": { 
			"16": "images/go_anon_16.png",
			"24": "images/go_anon_24.png",
			"32": "images/go_anon_32.png",
			"48": "images/go_anon_48.png",
			"128": "images/go_anon_128.png"
		},
		"permissions": ["activeTab"],
		"content_security_policy": "script-src 'self' https://ssl.google-analytics.com; object-src 'self'"
}